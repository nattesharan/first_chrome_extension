{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "1.0",
    "description": "Test extension for learning",
    "browser_action": {
        "default_icon": "icon.png"
    },
      "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ]
}